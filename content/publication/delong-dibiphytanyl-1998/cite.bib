 issn = {0099-2240, 1098-5336},
 journal = {Applied and Environmental Microbiology},
 language = {en},
 month = {March},
 number = {3},
 pages = {1133--1138},
 title = {Dibiphytanyl Ether Lipids in Nonthermophilic Crenarchaeotes},
 url = {https://journals.asm.org/doi/10.1128/AEM.64.3.1133-1138.1998},
