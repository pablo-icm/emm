	journal = {Science Advances},
	author = {Lara, Elena and Vaqué, Dolors and Sà, Elisabet Laia and Boras, Julia A. and Gomes, Ana and Borrull, Encarna and Díez-Vives, Cristina and Teira, Eva and Pernice, Massimo C. and Garcia, Francisca C. and Forn, Irene and Castillo, Yaiza M. and Peiró, Aida and Salazar, Guillem and Morán, Xosé Anxelu G. and Massana, Ramon and Catalá, Teresa S. and Luna, Gian Marco and Agustí, Susana and Estrada, Marta and Gasol, Josep M. and Duarte, Carlos M.},
