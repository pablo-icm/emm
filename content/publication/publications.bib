	journal = {Environmental Microbiology},
	author = {Wu, Wenxue and Logares, Ramiro and Huang, Bangqin and Hsieh, Chih‐hao},
	month = jan,
	year = {2017},
	pages = {287--300},
}

@article{mangot_accessing_2017,
