	pages = {3956--3964},
	file = {Texto completo:/Users/pablo/Zotero/storage/DRUIS7S2/Morán et al. - 2017 - Temperature regulation of marine heterotrophic prokaryotes increases latitudinally as a breach betwe.pdf:application/pdf},
}

@article{lara_unveiling_2017,
	title = {Unveiling the role and life strategies of viruses from the surface to the dark ocean},
