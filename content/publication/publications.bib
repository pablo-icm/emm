	number = {10},
	urldate = {2024-08-05},
	journal = {Global Change Biology},
	author = {Ayo, Begoña and Abad, Naiara and Artolozaga, Itxaso and Azua, Iñigo and Baña, Zuriñe and Unanue, Marian and Gasol, Josep M. and Duarte, Carlos M. and Iriberri, Juan},
